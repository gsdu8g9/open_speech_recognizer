<%= form_for @utterance, :html => { :class => 'form-horizontal form-model', :multipart => true } do |f| %>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
 	  <%= f.file_field :wav %>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  utterances_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>

